{
  "name": "serverless-custom-domain",
  "version": "0.4.1",
  "description": "Serverless plugin which creates a Custom Domain Name for the serverless service.",
  "main": "index.js",
  "scripts": {
    "test": "MOCHAWESOME_REPORTDIR=reports/mochawesome multi='mochawesome=- html-cov=reports/coverage/coverage.html' ./node_modules/mocha/bin/mocha -r blanket --reporter mocha-multi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Icenium/serverless-custom-domain.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Icenium/serverless-custom-domain/issues"
  },
  "homepage": "https://github.com/Icenium/serverless-custom-domain#readme",
  "dependencies": {
    "aws-sdk": "2.42.0"
  },
  "devDependencies": {
    "@types/chai": "4.0.2",
    "@types/mocha": "2.2.41",
    "blanket": "1.2.3",
    "chai": "4.1.1",
    "mocha": "2.5.3",
    "mocha-multi": "0.11.0",
    "mochawesome": "2.3.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib/*.js"
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  }
}
